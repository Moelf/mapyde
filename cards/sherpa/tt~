(run){

  %scales, tags for scale variations                        
  FSF:=1.; RSF:=1.; QSF:=1.;                                                                          
  SCALES LOOSE_METS{FSF*MU_F2}{RSF*MU_R2}{QSF*MU_Q2};       

  QCUT:=30.; NJET:=2; LJET:=0;

  % decays
  HARD_DECAYS=1
  STABLE[6]=0
  WIDTH[6]=0
  STABLE[24]=0

}(run)

(processes){
  Process 93 93 -> 6 -6
  CKKW sqr(QCUT/E_CMS)
  Order (*,0); 
  Integration_Error 0.1;
  End process;
}(processes)

(me){
  ME_SIGNAL_GENERATOR = Comix;
}(me)

(selector){
PT 5 15 E_CMS
PT -5 15 E_CMS
Mass 5 -5 30 10000000000
    }(selector)
